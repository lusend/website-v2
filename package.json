{
  "name": "lusend-website",
  "version": "1.7.0",
  "description": "LU Send's Website Files",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "release": "semantic-release",
    "build": "NODE_ENV=production eleventy",
    "dev": "NODE_ENV=development eleventy --serve --quiet",
    "clean": "rm -Rf dist",
    "debug:dev": "DEBUG=* NODE_ENV=development eleventy",
    "debug:prod": "DEBUG=* NODE_ENV=production eleventy",
    "predev": "npm run clean",
    "prebuild": "npm run clean"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.1",
    "@semantic-release/npm": "^8.0.2",
    "@semantic-release/release-notes-generator": "^10.0.2",
    "@tailwindcss/typography": "^0.4.1",
    "@types/jquery": "^3.5.8",
    "autoprefixer": "^10.4.0",
    "conventional-changelog-conventionalcommits": "^4.6.1",
    "daisyui": "^1.16.1",
    "html-minifier": "^4.0.0",
    "markdown-it-anchor": "^8.4.1",
    "markdown-it-attrs": "^4.1.0",
    "markdown-it-bracketed-spans": "^1.0.1",
    "markdown-it-container": "^3.0.0",
    "markdown-it-emoji": "^2.0.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-toc-done-right": "^4.2.0",
    "postcss": "^8.3.11",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.6",
    "prettier": "^2.4.1",
    "semantic-release": "^18.0.0",
    "tailwindcss": "^2.2.19"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits"
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits"
        }
      ],
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "package-lock.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): :bookmark: ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": [
            {
              "path": "dist/**"
            }
          ]
        }
      ]
    ],
    "repositoryUrl": "https://github.com/lusend/website"
  }
}
